{
  "name": "nft-marketplace-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "solhint": "solhint -f table contracts/**/*.sol",
    "prettier:solidity": "prettier --write contracts/**/*.sol",
    "hardhat:compile": "hardhat compile",
    "hardhat:mumbai": "hardhat run --network mumbai scripts/deploy.js",
    "hardhat:matic": "hardhat run --network matic scripts/deploy.js",
    "hardhat:bsctestnet": "hardhat run --network bsctestnet scripts/deploy.js",
    "hardhat:bsc": "hardhat run --network bsc scripts/deploy.js",
    "mint:mumbai": "hardhat run --network mumbai scripts/mint.js",
    "hardhat:verify": "hardhat verify --network mumbai process.env.VERIFY"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.4.1",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.0",
    "ipfs-http-client": "^55.0.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "tailwindcss": "^3.0.8"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.sol": "solhint -f table",
    "*.{js,css,md,sol}": "prettier --write"
  }
}
