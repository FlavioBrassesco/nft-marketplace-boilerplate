{
  "name": "nft-marketplace-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "solhint": "solhint -f table contracts/**/*.sol",
    "prettier:solidity": "prettier --write contracts/**/*.sol",
    "h:compile": "hardhat compile",
    "h:mumbai": "hardhat run --network mumbai scripts/deploy.js",
    "h:matic": "hardhat run --network matic scripts/deploy.js",
    "h:bsctestnet": "hardhat run --network bsctestnet scripts/deploy.js",
    "h:bsc": "hardhat run --network bsc scripts/deploy.js",
    "h:test": "hardhat test --network ganache",
    "h:verify": "hardhat verify --network mumbai process.env.VERIFY",
    "r:ether": "ganache-cli --defaultBalanceEther 10000"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.4",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.5.0",
    "@uniswap/v2-core": "^1.0.1",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "ethers-eip712": "^0.2.0",
    "hardhat": "^2.8.0",
    "ipfs-http-client": "^55.0.0",
    "modern-normalize": "^1.1.0",
    "mongoose": "^6.3.1",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.5",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "autoprefixer": "^10.4.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "ganache-cli": "^6.12.2",
    "hardhat-contract-sizer": "^2.5.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.sol": "solhint -f table",
    "*.{js,css,md,sol}": "prettier --write"
  }
}
